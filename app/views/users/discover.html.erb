<h1><%= @user.user_name %>'s Movie Discover Page</h1>

<%= button_to 'Discover Top Rated Movies', "/users/#{@user.id}/movies?q=top%20rated", method: :get  %> 

<%= form_with url: "/users/#{@user.id}/movies?q=keyword", method: :get, local: true do |f| %>
  <%= f.text_field :search, placeholder: 'Movie Title' %>
  <%= f.submit "Search" %>
<% end %>